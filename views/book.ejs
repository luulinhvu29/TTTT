<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet">

<div class="container col-8">
<table class="table table-bordered">
    <% for (let [index,book] of listbooks.entries() ) { %>
    <tr>
        <td><%= index+1 %> </td>
        <td><%= book.id %> </td>
        <td><%= book.city %> </td>
    </tr>
    <% } %>
  </table>
</div>